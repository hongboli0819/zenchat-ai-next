{
  "name": "@org/zip-folder-extractor",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:app": "vite build",
    "build:lib": "tsup src/core/index.ts --dts --format esm,cjs --sourcemap --clean",
    "preview": "vite preview"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.21",
    "lucide-react": "^0.474.0",
    "postcss": "^8.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.5",
    "tailwindcss": "^4.0.9",
    "tsup": "^8.5.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}

